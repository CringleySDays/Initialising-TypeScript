{
  "scripts": {
    "test": "mocha ./build/src/*.js",
    "build": "tsc",
    "start": "bun build/src/index.js",
    "dev:install": "bun install",
    "prod:install": "bun install --production",
    "prod": "bun run build && NODE_ENV=prod bun run start",
    "dev": "bun run build && NODE_ENV=dev bun run start",
    "dev:upgrade": "bun update",
    "prod:upgrade": "rm -r node_modules/ && rm -r bun.lockb && bun run prod:install",
    "uninstall": "rm -r node_modules/ && rm -r package-lock.json"
  },
  "dependencies": {
    "@prisma/client": "^5.5.2",
    "dotenv": "^16.3.1",
    "chalk": "^5.3.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.200",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "eslint": "^9.2.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "mocha": "^10.4.0",
    "prettier": "^3.0.3",
    "prettier-eslint": "^16.1.2",
    "prisma": "^5.5.2",
    "typescript": "^5.4.5",
    "@types/bun": "latest"
  },
  "private": true
}
